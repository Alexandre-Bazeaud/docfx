{
  "Version": "1.0.0",
  "UseMinimatch": false,
  "SignBatches": [
    {
      "MatchedPath": [
        "*.dll",
        "*.exe"
      ],
      "SigningInfo": {
        "Operations": [
          {
            "KeyCode": "CP-230012",
            "OperationCode": "SigntoolSign",
            "Parameters": {
              "OpusName": "Microsoft",
              "OpusInfo": "http://www.microsoft.com",
              "Append": "/as",
              "FileDigest": "/fd \"SHA256\"",
              "PageHash": "/NPH",
              "TimeStamp": "/tr \"http://rfc3161.gtm.corp.microsoft.com/TSS/HttpTspServer\" /td sha256"
            },
            "ToolName": "sign",
            "ToolVersion": "1.0"
          },
          {
            "KeyCode": "CP-230012",
            "OperationCode": "SigntoolVerify",
            "Parameters": {
              "VerifyAll": "/all"
            },
            "ToolName": "sign",
            "ToolVersion": "1.0"
          }
        ]
      }
    }
  ]
}